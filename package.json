    "repository": {
        "type": "git",
        "url": "https://github.com/esportsplus/reactivity"
    },
    "scripts": {
        "build": "tsc",
        "build:test": "pnpm build && vite build --config test/vite.config.ts",
        "-": "-",
        "prepare": "pnpm build",
        "prepublishOnly": "pnpm build"
    },
    "type": "module",
    "types": "build/index.d.ts",
    "version": "0.29.14"
